{
  "extends": [
    "markuplint:recommended-react"
  ],
  "parser": {
    "\\.[jt]sx$": "@markuplint/jsx-parser"
  },
  "specs": {
    "\\.[jt]sx$": "@markuplint/react-spec"
  },
  "pretenders": [
    {
      "selector": "Link",
      "as": "a"
    },
    {
      "selector": "Image",
      "as": "img"
    },
    {
      "selector": "Article",
      "as": "article"
    }
  ]
}
